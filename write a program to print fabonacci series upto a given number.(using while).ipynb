    "j=1\n",
    "sum=0\n",
    "sum_1=0\n",
    "while j<2:\n",
    "    sum = sum + j\n",
    "while j<=n:\n",
    "    sum_1 = sum + j\n",
    "    print('fabonacci series',sum_1)\n",
    "    j=j+2\n",
    "print('fabonacci series',sum_1)"
