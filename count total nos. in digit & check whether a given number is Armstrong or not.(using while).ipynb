    "n=int(input('n= '))\n",
    "sum=0\n",
    "p=0\n",
    "count=0\n",
    "p=n\n",
    "while p>0:\n",
    "    sum=sum + (p%10)**3\n",
    "    count=count + 1\n",
    "    p=p//10\n",
    "print('Total nos. in digit= ', count)\n",
    "if sum==n:\n",
    "    print('n is Armstrong no.')\n",
    "else:\n",
    "    print('n is not Armstrong no.')"
